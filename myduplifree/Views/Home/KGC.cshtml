@model List<myduplifree.Models.KGCModel>

@{
    ViewData["Title"]="KGC Page";
   
}


<div class="container">
        <h1>Cloud Files Form</h1>
        
        @using (Html.BeginForm("KGC", "Home", FormMethod.Post, new { @class = "row g-3" }))
        {
            <table class="table">
                <tr>
                    <td>
                        @Html.LabelFor(model => model.FileName, htmlAttributes: new { @class = "form-label" })
                        @Html.ValidationMessageFor(model => model.FileName, "", new { @class = "text-danger" })
                    </td>
                    <td>
                        @Html.TextBoxFor(model => model.FileName, new { @class = "form-control" })
                    </td>
                </tr>
                <tr>
                    <td>
                        @Html.LabelFor(model => model.FileLocation, htmlAttributes: new { @class = "form-label" })
                        @Html.ValidationMessageFor(model => model.FileLocation, "", new { @class = "text-danger" })
                    </td>
                    <td>
                        @Html.TextBoxFor(model => model.FileLocation, new { @class = "form-control" })
                    </td>
                </tr>
                <tr>
                    <td>
                        @Html.LabelFor(model => model.Publickey, htmlAttributes: new { @class = "form-label" })
                        @Html.ValidationMessageFor(model => model.Publickey, "", new { @class = "text-danger" })
                    </td>
                    <td>
                        @Html.TextBoxFor(model => model.Publickey, new { @class = "form-control" })
                    </td>
                </tr>
                <tr>
                    <td>
                        @Html.LabelFor(model => model.Updatedto, htmlAttributes: new { @class = "form-label" })
                        @Html.ValidationMessageFor(model => model.Updatedto, "", new { @class = "text-danger" })
                    </td>
                    <td>
                        @Html.TextBoxFor(model => model.Updatedto, new { @class = "form-control" })
                    </td>
                </tr>
                <tr>
                    <td>
                        @Html.LabelFor(model => model.Youarein, htmlAttributes: new { @class = "form-label" })
                        @Html.ValidationMessageFor(model => model.Youarein, "", new { @class = "text-danger" })
                    </td>
                    <td>
                        @Html.TextBoxFor(model => model.Youarein, new { @class = "form-control" }) <!-- Assuming Generate_Keys is a boolean property -->
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </td>
                </tr>
            </table>
        }
    </div>



